doctype 5
html(lang="en")
  head
    title= 'Plex Media Servers List'
    script(src='/public/js/jquery.js')
  body
    h1 Plex Media Servers
    p
      a(href='/logout') Sign out
    each server,i in plexServers
      div
        a(href='#{server.machineIdentifier}/sections/') #{server.name} - #{server.host}:#{server.port}